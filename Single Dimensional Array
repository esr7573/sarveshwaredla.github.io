package io.github.sarveshwaredla.arrays;

public class SingleDimensionalArray {

    public static void main(String[] args) { 
	
	int [] arr = new int[4];

	arr[0] = 10;
	arr[1] = 20;
	arr[2] = 30;
	arr[3] = 40;

	system.out.println("Element at index 0: "+ arr[0]);
	system.out.println("Element at index 0: "+ arr[1]);
	system.out.println("Element at index 0: "+ arr[2]);		
	system.out.println("Element at index 0: "+ arr[3]);
    }
}
