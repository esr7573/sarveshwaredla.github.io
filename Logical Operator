package io.github.sarveshwaredla.operators;

public class LogicalOperator {

    public static void main(String[] args) {
        int x=10;
        int y=80;
        int z=30;
        if(x>y && x>z){
            System.out.println("x is biggest");
        }
        else if(y>x && y>z){
            System.out.println("y is biggest");
        }
    }
}
