package io.github.sarveshwaredla.controstatements;

public class IfElseIf {

    public static void main(String[] args){
        String myname="sarvesh"; 
        if(myname=="harish"){
            System.out.println("my name is harish");
        }
        else if(myname=="rakesh"){
            System.out.println("my name is rakesh"); 
        }
        else if(myname=="sarvesh"){
            System.out.println("my name is sarvesh");
        }
        else {
            System.out.println("invalid name");
        }
    }
}
